{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "groq",
      "camelcase": "Groq",
      "title": "Groq",
      "scope": "source.groq",
      "file-types": [
        "groq"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^groq$",
      "class-name": "TreeSitterGroq"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the Groq query language used by the Sanity.io headless CMS",
    "authors": [
      {
        "name": "Kurt Steigleder",
        "url": "https://github.com/greymour"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-groq"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}